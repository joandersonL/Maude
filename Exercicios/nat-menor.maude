fmod NAT< is
  protecting NAT-ADD . protecting BOOLEAN .
  op _<_ : Nat Nat -> Boolean .
  vars M N : Nat .
  eq 0 < s(M) = true .
  eq M < 0 = false .
  eq s(M) < s(N) = M < N .
endfm
