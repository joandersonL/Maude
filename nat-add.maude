fmod NAT-ADD is
  sort Nat .
  op 0 : -> Nat [ctor] .
  op s : Nat -> Nat [ctor] .
  op _+_ : Nat Nat -> Nat .
  vars M N : Nat .
  *** Define the addition function recursively:

  eq s(M) + N = s(M + N) . *** [prec 55].
  eq 0 + M = M .
endfm
